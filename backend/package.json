{
  "name": "project",
  "version": "0.0.0",
  "engines": {
    "node": "12.11.0"
  },
  "description": "description",
  "author": "AleBlondin",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:run": "yarn typeorm migration:run",
    "typeorm": "ts-node node_modules/.bin/typeorm -f src/ormconfig.ts",
    "test:migration:run": "ts-node -r dotenv-safe/config node_modules/.bin/typeorm migration:run dotenv_config_path=.env.test",
    "schema:drop": "yarn typeorm schema:drop",
    "test:schema:drop": "ts-node -r dotenv-safe/config node_modules/.bin/typeorm schema:drop dotenv_config_path=.env.test",
    "reset": "rm -rf node_modules && npm install",
    "start": "node dist/src/main.js",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ts-node -r dotenv-safe/config node_modules/.bin/jest * dotenv_config_path=.env.test --maxWorkers=4",
    "test:watch": "jest --config jest.config.json --watch",
    "test:coverage": "ts-node -r dotenv-safe/config node_modules/.bin/jest * --coverage dotenv_config_path=.env.test --maxWorkers=4",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=4"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/typeorm": "^6.1.2",
    "@nestjsx/crud": "^4.0.0",
    "@nestjsx/crud-typeorm": "^4.0.0",
    "@types/dotenv-safe": "^5.0.3",
    "@types/node": "^12.11.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.4",
    "dotenv-safe": "^8.1.0",
    "emoji-js": "^3.5.0",
    "lodash": "^4.17.15",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "ts-node": "^8.2.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/emoji-js": "^3.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.121",
    "@types/passport-jwt": "^3.0.1",
    "@types/request-promise": "^4.1.45",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
